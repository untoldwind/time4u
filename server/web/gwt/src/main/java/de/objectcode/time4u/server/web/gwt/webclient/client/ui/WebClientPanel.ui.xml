<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:twc="urn:import:de.objectcode.time4u.server.web.gwt.webclient.client.ui"
	xmlns:tu="urn:import:de.objectcode.time4u.server.web.gwt.utils.client.ui">

	<g:SplitLayoutPanel>
		<g:west size="300">
			<g:SplitLayoutPanel ui:field="westPanel">
				<g:north size="200">
					<twc:ProjectTree ui:field="projectTree" />
				</g:north>
				<g:center>
					<twc:TaskList ui:field="taskList" />
				</g:center>
				<g:south size="210">
					<twc:CalendarView ui:field="calendarView" />
				</g:south>
			</g:SplitLayoutPanel>
		</g:west>
		<g:center>
			<twc:WorkItemList />
		</g:center>
		<g:south size="210">
			<g:SplitLayoutPanel>
				<g:center>
					<twc:StatisticsView />
				</g:center>
				<g:east size="200">
					<twc:PunchView />
				</g:east>
			</g:SplitLayoutPanel>
		</g:south>
	</g:SplitLayoutPanel>
</ui:UiBinder>