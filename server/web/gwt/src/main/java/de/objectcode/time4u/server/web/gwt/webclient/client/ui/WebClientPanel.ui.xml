<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:twc="urn:import:de.objectcode.time4u.server.web.gwt.webclient.client.ui"
	xmlns:tu="urn:import:de.objectcode.time4u.server.web.gwt.utils.client.ui">

	<g:SplitLayoutPanel ui:field="mainPanel">
		<g:west size="300">
			<g:SplitLayoutPanel>
				<g:north size="200">
					<twc:ProjectTree />
				</g:north>
				<g:center>
					<twc:TaskList />
				</g:center>
				<g:south size="200">
					<twc:CalendarView />
				</g:south>
			</g:SplitLayoutPanel>
		</g:west>
		<g:center>
			<twc:WorkItemList />
		</g:center>
	</g:SplitLayoutPanel>
</ui:UiBinder>