<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gd="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:tr="urn:import:de.objectcode.time4u.server.web.gwt.report.client.ui"
	xmlns:tu="urn:import:de.objectcode.time4u.server.web.gwt.utils.client.ui">

	<g:DockLayoutPanel>
		<g:north size="40">
			<g:VerticalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="personTeam">Person</g:RadioButton>
					<g:RadioButton name="personTeam">Team</g:RadioButton>
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:Label>From:</g:Label> <gd:DateBox/>
					<g:Label>Until:</g:Label> <gd:DateBox/>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:north>
		<g:center>
			<g:ScrollPanel>
				<tr:InteractiveReportPanel.InteractiveReportTable ui:field="reportTable" width="100%" />
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>