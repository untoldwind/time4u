<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="resources"
		type="de.objectcode.time4u.server.web.gwt.main.client.MainClientBundle" />

	<g:DockLayoutPanel>
		<g:north size="25">
			<g:HorizontalPanel>
				<g:PushButton ui:field="newProject">
					<g:upFace image="{resources.newProject}" />
				</g:PushButton>
				<g:PushButton ui:field="editProject" enabled="false">
					<g:upFace image="{resources.editProject}" />
				</g:PushButton>
				<g:PushButton ui:field="deleteProject" enabled="false">
					<g:upFace image="{resources.deleteProject}" />
				</g:PushButton>
			</g:HorizontalPanel>
		</g:north>
		<g:center>
			<g:ScrollPanel>
				<g:Tree ui:field="projectTree" width="100%" />
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>
 